  "description": "",
  "devDependencies": {
    "@commitlint/cli": "^7.5",
    "@commitlint/config-angular": "^7.5",
    "@types/node": "^11.10",
    "concurrently": "^4.1",
    "fs-extra": "^7.0.1",
    "husky": "^1.3",
    "jest": "^24.1",
    "prettier": "^1.16",
    "stylelint": "^9.10",
    "stylelint-config-recommended-scss": "^3.2",
    "stylelint-order": "^2.1",
    "stylelint-scss": "^3.5",
    "supertest": "^3.4",
    "ts-jest": "^24.0",
    "ts-node": "^8.0",
    "tslint": "^5.13",
    "tslint-consistent-codestyle": "^1.15",
    "tslint-eslint-rules": "^5.4",
    "typescript": "^3.3"
  },
  "engines": {
    "node": "^9 || ^10"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -x @commitlint/config-angular -e",
      "pre-commit": "yarn format"
    }
  },
  "license": "MIT",
  "name": "@dl2/project-name",
  "scripts": {
    "build": "node tools/copy-files && tsc",
    "build:fast": "node tools/copy-files && tsc",
    "format": "concurrently yarn:~prettier:* && yarn lint --fix",
    "lint": "tslint -p tsconfig.json",
    "prebuild": "yarn test",
    "prestart": "yarn ~setup",
    "test": "NODE_ENV=test jest",
    "~prettier": "prettier --ignore-path .gitignore --arrow-parens always --print-width 84 --trailing-comma all --write",
    "~prettier:others": "yarn ~prettier '**/*.?(js|json)'",
    "~prettier:typescript": "yarn ~prettier --parser typescript '**/*.ts'",
    "~setup": "yarn"
  },
  "version": "0.0.0"
}

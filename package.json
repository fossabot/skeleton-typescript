  "license": "MIT",
  "name": "@dl2/project-name",
  "scripts": {
    "build": "tsc",
    "commitmsg": "commitlint -x @commitlint/config-angular -e",
    "format": "yarn ~prettier 'src/**/*.ts' && yarn lint --fix",
    "lint": "tslint -p tsconfig.json",
    "prebuild": "yarn test",
    "precommit": "yarn format && yarn test",
    "prestart": "yarn ~setup",
    "test": "NODE_ENV=test jest",
    "~prettier": "prettier --arrow-parens always --parser typescript --print-width 84 --trailing-comma all --write",
    "~setup": "yarn"
  },
  "version": "1.0.0"
}
